    Попытка
ДО = ОбъектВх.ПолучитьОбъект();
Сообщить(ДО);
СпрКат = Справочники.тсЗначенияСвойствОбъектов;
ЗначКат = СпрКат.НайтиПоНаименованию("Архив");
КодКат = "0181";
ОбъектыСервер.ЗаписатьЗначениеСвойства(ОбъектВх, КодКат, ЗначКат); 
Сообщить(ЗначКат);
СпрКат = Справочники.тсЗначенияСвойствОбъектов;
ЗначКат = СпрКат.НайтиПоНаименованию("Банкротство");
КодПрич = "0182";
ОбъектыСервер.ЗаписатьЗначениеСвойства(ОбъектВх, КодПрич, ЗначКат); 
Сообщить(ЗначКат);
    Значение = "Выполнено";
Исключение
    Значение = "Ошибка";
КонецПопытки;



